<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>operationServlet</servlet-name>
        <servlet-class>operationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>controleLoginCorrentista</servlet-name>
        <servlet-class>controleLoginCorrentista</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>controleCriacaoCorrentista</servlet-name>
        <servlet-class>controleCriacaoCorrentista</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>controleLoginFunc</servlet-name>
        <servlet-class>controleLoginFunc</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>controleExtrato</servlet-name>
        <servlet-class>controleExtrato</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>controleSaldo</servlet-name>
        <servlet-class>controleSaldo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>controleTransferencia</servlet-name>
        <servlet-class>controleTransferencia</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>controleCriacaoFuncionario</servlet-name>
        <servlet-class>controleCriacaoFuncionario</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>operationServlet</servlet-name>
        <url-pattern>/operationServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>controleLoginCorrentista</servlet-name>
        <url-pattern>/controleLoginCorrentista</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>controleCriacaoCorrentista</servlet-name>
        <url-pattern>/controleCriacaoCorrentista</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>controleLoginFunc</servlet-name>
        <url-pattern>/controleLoginFunc</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>controleExtrato</servlet-name>
        <url-pattern>/controleExtrato</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>controleSaldo</servlet-name>
        <url-pattern>/controleSaldo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>controleTransferencia</servlet-name>
        <url-pattern>/controleTransferencia</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>controleCriacaoFuncionario</servlet-name>
        <url-pattern>/controleCriacaoFuncionario</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
